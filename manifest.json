{
  "manifest_version": 3,
  "name": "PoliceAC",
  "version": "0.7.1",
  "description": "",
  "permissions": ["activeTab"],
  "icons": {},
  "content_scripts": [
    {
      "matches": ["https://psy.league17.ru/*"],
      "js": ["src/js/Buttons.js", "src/js/Police.js", "src/js/Event.js", "src/js/Clicker.js"],
      "css": ["src/css/all.css", "src/css/style.css"],
      "type": "module"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/assets/fa-light-300.woff2",
        "src/assets/fa-solid-900.woff2",
        "src/assets/fa-duotone-900.woff2",
        "src/assets/fa-thin-100.woff2"
      ],
      "matches": ["https://psy.league17.ru/*"]
    }
  ]
}
